package Person;

public class Test {

	public static void main(String[] args){
		Person taro = new Person();
		taro.name ="山田太郎";
		taro.age =20;


		System.out.println(taro.name);
		System.out.println(taro.age);


		public static void main(String[] args){
			Person jiro = new Person();
			jiro.name ="木村次郎";
			jiro.age =18;


			System.out.println(joro.name);
			System.out.println(jiro.age);
	}
}
