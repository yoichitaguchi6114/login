package Person;


public class Test {

	public static void main(String[] args){
		Person taro = new Person();
		taro.name ="山田太郎";
		taro.age =20;


		System.out.println(taro.name);
		System.out.println(taro.age);


		Person jiro = new Person();
		jiro.name ="鈴木次郎";
		jiro.age =18;


		System.out.println(jiro.name);
		System.out.println(jiro.age);

	}

}